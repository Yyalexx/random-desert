39 \u0444\u0430\u0440\u0448. \u041f\u043e\u043f\u0440\u043e\u0431\u0443\u0439\u0442\u0435 \u043f\u043e\u044d\u043a\u0441\u043f\u0435\u0440\u0438\u043c\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0441 \u0441\u043e\u0447\u0435\u0442\u0430\u043d\u0438\u0435\u043c \u043c\u044f\u0441\u0430 \u0441\u0432\u0438\u043d\u0438\u043d\u044b \u0438 \u0433\u043e\u0432\u044f\u0434\u0438\u043d\u044b 50/50. \u041b\u0438\u0431\u043e \u043c\u043e\u0436\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u043e \u043e\u0431\u043e\u0439\u0442\u0438\u0441\u044c \u043a\u0443\u0440\u0438\u043d\u044b\u043c. \u041d\u0430\u043a\u043b\u0430\u0434\u044b\u0432\u0430\u0435\u043c \u0433\u043e\u0442\u043e\u0432\u044b\u0439 \u0444\u0430\u0440\u0448 \u0432 \u0433\u043b\u0443\u0431\u043e\u043a\u0443\u044e \u043c\u0438\u0441\u043a\u0443, \u0440\u0430\u0437\u0431\u0438\u0432\u0430\u0435\u043c \u044f\u0439\u0446\u0430, \u043f\u043e \u0432\u043a\u0443\u0441\u0443 \u043f\u043e\u0441\u044b\u043f\u0430\u0435\u043c \u0441\u043e\u043b\u044c\u044e \u0438 \u0447\u0451\u0440\u043d\u044b\u043c \u043c\u043e\u043b\u043e\u0442\u044b\u043c \u043f\u0435\u0440\u0446\u0435\u043c. \u0421\u043c\u0435\u0448\u0438\u0432\u0430\u0435\u043c \u0440\u0443\u043a\u043e\u0439 \u043b\u0438\u0431\u043e \u0431\u043e\u043b\u044c\u0448\u043e\u0439 \u043b\u043e\u0436\u043a\u043e\u0439. \u0417\u0430\u0442\u0435\u043c \u0448\u0438\u043d\u043a\u0443\u0435\u043c \u043a\u0430\u043f\u0443\u0441\u0442\u0443 \u043c\u0430\u043b\u0435\u043d\u044c\u043a\u0438\u043c\u0438 \u043b\u043e\u043c\u0442\u0438\u043a\u0430\u043c\u0438. \u0414\u043b\u044f \u0442\u043e\u0433\u043e \u0447\u0442\u043e\u0431\u044b \u043a\u0430\u043f\u0443\u0441\u0442\u0430 \u043f\u0440\u0438\u043e\u0431\u0440\u0435\u043b\u0430 \u043d\u0435\u0436\u043d\u044b\u0439 \u0432\u043a\u0443\u0441, \u043d\u0443\u0436\u043d\u043e \u0440\u0430\u0437\u043c\u044f\u0442\u044c \u0440\u0443\u043a\u0430\u043c\u0438. \u041a\u0430\u043a \u0430\u043b\u044c\u0442\u0435\u0440\u043d\u0430\u0442\u0438\u0432\u0443 \u043c\u043e\u0436\u043d\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u0442\u043e\u043b\u043a\u0430\u0447\u0438\u043a \u0434\u043b\u044f \u043a\u0430\u0440\u0442\u043e\u0444\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u044e\u0440\u0435. \u041a\u043e\u0433\u0434\u0430 \u043a\u0430\u043f\u0443\u0441\u0442\u0430 \u0431\u0443\u0434\u0435\u0442 \u0433\u043e\u0442\u043e\u0432\u0430, \u0442\u043e \u0432\u044b\u043b\u043e\u0436\u0438\u0442\u0435 \u0435\u0451 \u0432 \u043d\u0430\u0448 \u0444\u0430\u0440\u0448 \u0438 \u043f\u0435\u0440\u0435\u043c\u0435\u0448\u0430\u0439\u0442\u0435 \u0432\u0441\u0435 \u0438\u043d\u0433\u0440\u0435\u0434\u0438\u0435\u043d\u0442\u044b.\u041a\u043e\u0433\u0434\u0430 \u043d\u0430\u0448\u0430 \u043c\u0430\u0441\u0441\u0430 \u0431\u0443\u0434\u0435\u0442 \u0433\u043e\u0442\u043e\u0432\u0430, \u0442\u043e \u043c\u043e\u043a\u0440\u044b\u043c\u0438 \u0440\u0443\u043a\u0430\u043c\u0438 \u0444\u043e\u0440\u043c\u0438\u0440\u0443\u0435\u043c \u0444\u043e\u0440\u043c\u0443 \u0434\u043b\u044f \u043d\u0430\u0448\u0438\u0445 \u043b\u0435\u043d\u0438\u0432\u044b\u0445 \u0433\u043e\u043b\u0443\u0431\u0446\u043e\u0432. \u0427\u0442\u043e\u0431\u044b \u0433\u043e\u043b\u0443\u0431\u0446\u044b \u043f\u043e\u043b\u0443\u0447\u0438\u043b\u0438\u0441\u044c \u043f\u043b\u043e\u0442\u043d\u044b\u043c\u0438, \u0442\u043e \u043d\u0443\u0436\u043d\u043e \u043b\u0430\u0434\u043e\u043d\u044f\u043c\u0438 \u0443\u0442\u0440\u0430\u043c\u0431\u043e\u0432\u0430\u0442\u044c \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u044c \u043a\u0430\u0436\u0434\u043e\u0433\u043e \u0433\u043e\u043b\u0443\u0431\u0446\u0430. \u0411\u0435\u0440\u0451\u043c \u0440\u0435\u0448\u0451\u0442\u043a\u0443-\u0433\u0440\u0438\u043b\u044c \u0438 \u043f\u043e\u043c\u0435\u0449\u0430\u0435\u043c \u043b\u0435\u043d\u0438\u0432\u044b\u0435 \u0433\u043e\u043b\u0443\u0431\u0446\u044b \u0432 \u043f\u0430\u0440\u043e\u0432\u0430\u0440\u043a\u0443. \u0421\u0442\u0430\u0432\u0438\u043c \u043f\u0440\u0438\u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c\u0441\u044f \u043e\u043a\u043e\u043b\u043e 30 \u043c\u0438\u043d\u0443\u0442. \u041f\u043e\u0441\u043b\u0435 \u0443\u043a\u0430\u0437\u0430\u043d\u043d\u043e\u0433\u043e \u0432\u0440\u0435\u043c\u0435\u043d\u0438 \u043d\u0430\u0448\u0438 \u0441\u043e\u0447\u043d\u044b\u0435 \u043b\u0435\u043d\u0438\u0432\u044b\u0435 \u0433\u043e\u043b\u0443\u0431\u0446\u044b \u043c\u043e\u0436\u043d\u043e \u0440\u0430\u0441\u043a\u043b\u0430\u0434\u044b\u0432\u0430\u0442\u044c \u043f\u043e \u0442\u0430\u0440\u0435\u043b\u043a\u0430\u043c \u0438 \u043f\u043e\u0434\u0430\u0432\u0430\u0442\u044c \u043d\u0430 \u0441\u0442\u043e\u043b. \u0423\u043a\u0440\u0430\u0441\u044c\u0442\u0435 \u0437\u0435\u043b\u0435\u043d\u044c\u044e \u0438 \u0434\u043e\u0431\u0430\u0432\u044c\u0442\u0435 \u0441\u043c\u0435\u0442\u0430\u043d\u044b.'}]"